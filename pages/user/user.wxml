                                        </view>
                                        <view class="flex-grow-1 flex-y-center">{{item.name}}</view>
                                    </view>
                                    <view class="flex-grow-0 flex-y-center">
                                        <image class="option-jisntou" src="{{__wxapp_img.store.jiantou_r.url}}"></image>
                                    </view>
                                </view>
                            </navigator>
